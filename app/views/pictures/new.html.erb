<h1>Add A Picture</h1>
<%= form_for Picture.new do |p|  %>
<%= p.text_field(:url, placeholder: 'URL')%>
<%= p.text_field(:comments, placeholder: 'Leave a comment')%>
<%= p.text_field(:location, placeholder: 'Location')%>
<%# p.hidden_field :user_id, :value => @user.id %>
<%= p.submit('Submit') %>
<%end%>
